     const pdfUrl = \"/VijayRamesh.pdf\"; // Accessing directly from public folder\n            const link = document.createElement(\"a\");\n            link.href = pdfUrl;\n            link.download = \"VijayRamesh.pdf\"; // Change the name if needed\n            link.click();\n        },\n        startTyping() {\n            this.typeInterval = setInterval(() => {\n                if (this.typeIndex < this.role.length) {\n                    this.typeRole += this.role.charAt(this.typeIndex)\n                    this.typeIndex++\n                } else {\n                    clearInterval(this.typeInterval)\n                }\n            }, 250)\n        },\n    },\n    mounted() {\n        this.startTyping()\n    }\n}\n</script>\n<style scoped>\n.SwipeDown {\n    position: absolute;\n    bottom: 1%;  \n}\n\n.profile-section {\n    height: 100vh;\n    /* background-color: rgb(242, 245, 248); */\n    position: relative;\n}\n\n.blink-cursor {\n    animation: blink 1s step-end infinite;\n}\n\n.Main-Color-Ball {\n    filter: blur(20px);\n    background-color: rgb(0, 217, 255);\n    border-radius: 50%;\n    opacity: 0.05;\n    position: absolute;\n}\n\n.color-ball1 {\n    top: 70%;\n    left: 20%;\n    height: 150px;\n    width: 150px;\n}\n\n.color-ball2 {\n    top: 5%;\n    left: -10%;\n    height: 500px;\n    width: 500px;\n}\n\n.color-ball3 {\n    top: 50%;\n    right: -5%;\n    height: 350px;\n    width: 350px;\n}\n\n.color-ball4 {\n    top: 25%;\n    right: 30%;\n    height: 500px;\n    width: 500px;\n}\n</style>"]}]}